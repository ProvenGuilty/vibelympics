{
  "name": "weakest-lynx",
  "version": "1.0.0",
  "description": "Supply chain security auditor",
  "type": "module",
  "bin": {
    "lynx": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "build:cli": "tsc -p tsconfig.cli.json",
    "start": "node dist/server/index.js",
    "cli": "tsx src/cli/index.ts",
    "lynx": "tsx src/cli/index.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/d3": "^7.4.3",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "clsx": "^2.0.0",
    "commander": "^14.0.2",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ora": "^9.0.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/xml2js": "^0.4.14",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.21"
  }
}
